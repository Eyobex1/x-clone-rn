{
  "expo": {
    "name": "Eyob Twitter",
    "slug": "mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.jpg", // launcher icon
    "scheme": "mobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/icon.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#f54275"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.eyob.eyobtweet",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "clerkPublishableKey": "pk_test_bWFpbi1yYXZlbi04LmNsZXJrLmFjY291bnRzLmRldiQ",
      "router": {},
      "eas": {
        "projectId": "24aba022-f9bb-4d03-9283-f1b375b95624"
      }
    }
  }
}
